{
  "require": {
    "krokedil/settings-page": "@dev"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^2.3",
    "php-stubs/wordpress-stubs": "^6.1",
    "php-stubs/woocommerce-stubs": "^7.2",
    "wpify/scoper": "3.1.3"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "wpify/scoper": true
    }
  },
  "extra": {
    "wpify-scoper": {
      "prefix": "KrokedilKlarnaPaymentsDeps",
      "folder": "dependencies",
      "autorun": true,
      "globals": [
        "wordpress",
        "woocommerce"
      ]
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../krokedil/settings-page"
    }
  ],
  "minimum-stability": "dev"
}
